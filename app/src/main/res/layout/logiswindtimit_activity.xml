<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    <RelativeLayout 
    android:layout_width="match_parent"
    android:layout_height="50dp"
    android:background="@color/white"
    >
    <ImageView
        android:id="@+id/topButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:layout_centerVertical="true"
        android:layout_marginLeft="20dp"
        android:src="@drawable/blake" />

    <TextView
        android:id="@+id/topTv"
        android:layout_width="130dp"
        android:layout_height="40dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
       android:background="@drawable/logose" />

<ImageView
    android:id="@+id/rigitButton"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentRight="true"
    android:layout_centerVertical="true"
    android:layout_marginRight="20dp"
    android:visibility="gone"
    android:src="@drawable/new_reghtbg" />

<View
    android:layout_width="match_parent"
    android:layout_height="1dp"
    android:layout_alignParentBottom="true"
    android:background="@color/zilan" />

</RelativeLayout>
<RelativeLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    >
   <LinearLayout
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                   android:orientation="vertical"
                   android:background="@drawable/new_lo_bg">
                   <LinearLayout
           android:id="@+id/next_layout"
       android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:background="@color/huise"
      android:orientation="vertical"
       >
                <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                 android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="5dp"
                 android:background="@drawable/corners_bg"
                >
                     <LinearLayout
                    android:id="@+id/lout111"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:visibility="gone"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:layout_marginLeft="20dp"
                        android:text="发件人"
                        android:textColor="@color/black"
                        android:textSize="14sp" />

                </LinearLayout>
                   <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@+id/lout111"
                            android:visibility="gone"
                        android:background="@color/grayview" />
                      <RelativeLayout
                    android:id="@+id/lout2"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/lout111"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp" >

                    <EditText
                        android:id="@+id/et_name"
                        android:layout_width="100dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@+id/iv_names"
                        android:background="@null"
                        android:hint="寄件人姓名"
                        android:imeOptions="actionNext"
                        android:layout_centerVertical="true"
                        android:paddingLeft="5dp"
                        android:singleLine="true"
                        android:textSize="12sp" />
                    <View
                        android:id="@+id/shu"
                        android:layout_width="1dp"
                        android:layout_height="30dp"
                         android:layout_toRightOf="@id/et_name"
                        android:background="@color/grayview" />
<EditText
                        android:id="@+id/et_tel"
                        android:layout_width="120dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@id/et_name"
                        android:background="@null"
                        android:hint="寄件人手机号"
                        android:inputType="phone"
                        android:maxLength="13"
                        android:layout_centerVertical="true"
                        android:paddingLeft="5dp"
                        android:textSize="12sp" />
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_below="@id/et_name"
                          android:layout_marginTop="5dp"
                          android:visibility="gone"
                        android:background="@color/grayview" />

                    <ImageView
                        android:id="@+id/imageView3"
                        android:layout_width="5dp"
                        android:layout_height="5dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/red_xinghao"
                               android:visibility="invisible" />

                    <ImageView
                        android:id="@+id/iv_names"
                        android:layout_width="25dp"
                        android:layout_height="18dp"
                        android:layout_centerVertical="true"
                        android:background="@drawable/lianxiren_bg" />

                    <ImageView
                        android:id="@+id/iv_senderList"
                        android:layout_width="20dp"
                        android:layout_height="23dp"
                        android:layout_alignParentRight="true"
                        android:layout_alignTop="@+id/iv_names"
                        android:layout_marginRight="15dp"
                         android:layout_centerVertical="true"
                        android:background="@drawable/tongxu_bgs" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/lout3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/lout2"
                       android:layout_marginLeft="10dp"
                       android:visibility="gone"
                    android:layout_marginTop="5dp" >
<ImageView
                        android:id="@+id/imageView121"
                        android:layout_width="5dp"
                        android:layout_height="5dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                          android:src="@drawable/red_xinghao" 
                            android:visibility="invisible" 
                        />
                    <ImageView
                        android:id="@+id/iv_tel"
                        android:layout_width="25dp"
                      android:layout_height="18dp"
                        android:layout_centerVertical="true"
                        android:background="@drawable/phone_bg" />

                    

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                         android:layout_marginTop="5dp"
                          android:visibility="gone"
                        android:background="@color/grayview" />
                </RelativeLayout>
                    </RelativeLayout>
       <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                 android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="8dp"
                 android:background="@drawable/corners_bg"
                >

                <LinearLayout
                    android:id="@+id/loutnext4"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:background="#ffffff"
                    android:visibility="gone"
                    android:orientation="horizontal" >

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_vertical"
                        android:text="收件人"
                        android:textSize="14sp" />
                </LinearLayout>
                 <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_alignParentLeft="true"
                         android:visibility="gone"
                        android:layout_below="@+id/loutnext4"
                        android:background="@color/grayview" />
                <RelativeLayout
                    android:id="@+id/lout5"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="3dp" >
     <ImageView
                        android:id="@+id/imageView5"
                        android:layout_width="5dp"
                        android:layout_height="5dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/red_xinghao" 
                          android:visibility="invisible" />
                    <ImageView
                        android:id="@+id/iv_01"
                        android:layout_width="25dp"
                       android:layout_height="18dp"
                        android:layout_centerVertical="true"
                        android:background="@drawable/lianxiren_bg" />

                    <EditText
                        android:id="@+id/et_add_name"
                        android:layout_width="100dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@id/iv_01"
                        android:background="@null"
                        android:hint="收件人姓名"
                        android:imeOptions="actionNext"
                        android:paddingLeft="5dp"
                        android:layout_centerVertical="true"
                        android:singleLine="true"
                        android:textSize="12sp" />
 <View
                        android:id="@+id/shu1"
                        android:layout_width="1dp"
                        android:layout_height="30dp"
                         android:layout_toRightOf="@id/et_add_name"
                        android:background="@color/grayview" />
                    <EditText
                        android:id="@+id/et_add_tel"
                        android:layout_width="120dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@id/et_add_name"
                        android:background="@null"
                        android:hint="收件人手机号"
                        android:inputType="phone"
                        android:maxLength="13"
                          android:layout_centerVertical="true"
                        android:paddingLeft="5dp"
                        android:textSize="12sp" />
                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                             android:layout_marginTop="5dp"
                        android:layout_below="@id/et_add_name"
                        android:visibility="gone"
                        android:background="@color/grayview" />

                    <ImageView
                        android:id="@+id/iv_receiverList"
                        android:layout_width="20dp"
                        android:layout_height="23dp"
                        android:layout_alignParentRight="true"
                        android:layout_alignTop="@+id/iv_01"
                        android:layout_marginRight="15dp"
                        android:layout_centerVertical="true"
                        android:background="@drawable/tongxu_bgs" />

                   

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/lout6"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/lout5"
                     android:layout_marginLeft="10dp"
                     android:visibility="gone"
                    android:layout_marginTop="5dp" >
<ImageView
                        android:id="@+id/imageView11"
                        android:layout_width="5dp"
                        android:layout_height="5dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/red_xinghao" 
                          android:visibility="invisible" />
                    <ImageView
                        android:id="@+id/iv_02"
                        android:layout_width="25dp"
                        android:layout_height="18dp"
                        android:layout_centerVertical="true"
                        android:background="@drawable/phone_bg" />

                    

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                             android:layout_marginTop="5dp"
                               android:visibility="gone"
                        android:background="@color/grayview" />
                </RelativeLayout>

                  <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/lout6"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp" >
                     </RelativeLayout>
                     
            </RelativeLayout>   
             <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                 android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="8dp"
                 android:background="@drawable/corners_bg"
                >
            <RelativeLayout
                    android:id="@+id/lout7"
                    android:layout_width="match_parent"
                    
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="5dp" >
         <ImageView
                        android:id="@+id/imageView7"
                        android:layout_width="5dp"
                        android:layout_height="5dp"
                        android:layout_alignParentLeft="true"
                        android:layout_centerVertical="true"
                        android:src="@drawable/red_xinghao" 
                          android:visibility="invisible" />
                    <ImageView
                        android:id="@+id/iv_05"
                        android:layout_width="25dp"
                        android:layout_height="18dp"
                        android:layout_centerVertical="true"
                        android:background="@drawable/wupin_bg" />

                    <EditText
                        android:id="@+id/et_goodname"
                        android:layout_width="120dp"
                        android:layout_height="30dp"
                        android:layout_marginLeft="10dp"
                        android:layout_toRightOf="@id/iv_05"
                        android:background="@null"
                        android:hint="物品名称"
                        android:imeOptions="actionNext"
                        android:paddingLeft="5dp"
                        android:singleLine="true"
                        android:layout_centerVertical="true"
                        android:textSize="12sp" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                             android:layout_marginTop="5dp"
                        android:layout_below="@id/et_goodname"
                        android:visibility="gone"
                        android:background="@color/dividerColor" />
                </RelativeLayout>
                </RelativeLayout>
                 <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                 android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="8dp"
                 android:background="@drawable/corners_bg"
                >

            <RelativeLayout
                    android:id="@+id/lout123"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@id/lout10"
                    android:layout_marginLeft="10dp"
                    android:layout_marginRight="10dp"
                    android:layout_marginBottom="10dp"
                     >
                    <CheckBox
                        android:id="@+id/checkbox_baoss"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                       
                        android:textSize="12sp"
                        android:textColor="@color/black"
                        android:text="物品投保" />
                      	<View
                      	    android:id="@+id/v_pps"
                      	    android:layout_width="120dp"
                      	    android:layout_height="1dp"
                      	    android:layout_below="@+id/edt_prices"
                      	    android:background="@color/grayview" 
                      	    />

                      	<EditText
                      	    android:id="@+id/edt_prices"
                      	    android:layout_width="wrap_content"
                      	    android:layout_height="wrap_content"
                      	    android:layout_alignParentLeft="true"
                      	    android:layout_below="@+id/checkbox_baoss"
                      	    android:background="@null"
                      	    android:paddingLeft="2dp"
                      	    
                      	    android:ems="10"
                      	    android:hint="  请填写物品价值(元)"
                      	    android:inputType="number"
                      	    android:textSize="12sp" />

                      	<TextView
                      	    android:id="@+id/toubaoxuzhi"
                      	    android:layout_width="wrap_content"
                      	    android:layout_height="wrap_content"
                      	    android:layout_alignBaseline="@+id/checkbox_baoss"
                      	    android:layout_alignBottom="@+id/checkbox_baoss"
                      	    android:layout_toRightOf="@+id/checkbox_baoss"
                      	    android:layout_marginLeft="5dp"
                      	    android:textColor="@color/orange"
                      	    android:textSize="12sp"
                      	    android:text="(投保须知)" />
        <View
                    android:layout_width="1dp"
                    android:layout_height="35dp"
                    
                    android:layout_centerVertical="true"
                    android:layout_centerHorizontal="true"
                    android:background="@color/dividerColor" />
   	<CheckBox
   	    android:id="@+id/checkbox_diashou"
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
   	    android:layout_alignParentRight="true"
   	    android:layout_alignParentTop="true"
   	    android:layout_marginRight="50dp"
   	    android:text="请代收货款"
   	    android:textColor="@color/black"
   	    android:textSize="12sp" />
   	<TextView
   	    android:id="@+id/daitoubao"
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
   	    android:layout_alignBaseline="@+id/checkbox_diashou"
   	    android:layout_alignBottom="@+id/checkbox_diashou"
   	    android:layout_alignParentRight="true"
   	    android:text="(代收须知)"
   	    android:layout_marginRight="10dp"
   	    android:textColor="@color/orange"
   	    android:visibility="gone"
   	    android:textSize="12sp" />

   	<EditText
   	    android:id="@+id/edt_daipric"
   	    android:layout_width="wrap_content"
   	    android:layout_height="wrap_content"
   	    android:layout_alignParentRight="true"
   	    android:layout_below="@+id/checkbox_diashou"
   	    android:background="@null"
   	    android:ems="8"
   	    android:inputType="number"
   	    android:paddingLeft="1dp"
   	    android:hint=" 代收货款(元)"
   	    android:textSize="12sp" >

   	    <requestFocus />
   	</EditText>

   	<View
   	    android:id="@+id/vi_dai"
   	    android:layout_width="120dp"
   	    android:layout_height="1dp"
   	    android:layout_alignParentRight="true"
   	    android:layout_below="@+id/edt_daipric"
   	    android:background="@color/grayview" />

   	<ImageView
   	    android:id="@+id/imagebi"
   	    android:layout_width="5dp"
   	    android:layout_height="5dp"
   	    android:layout_alignParentLeft="true"
   	    android:layout_alignTop="@+id/edt_prices"
   	    android:visibility="gone"
   	    android:src="@drawable/red_xinghao" />
                </RelativeLayout>

             </RelativeLayout>
             <RelativeLayout
                   android:id="@+id/lout773"
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                      android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                     android:background="@drawable/corners_bg"
                   android:layout_marginTop="5dp" >

               <EditText
                   android:id="@+id/et_good_detail"
                   android:layout_width="match_parent"
                   android:layout_height="30dp"
                   android:layout_centerVertical="true"
                   android:layout_toRightOf="@+id/ee"
                   android:background="@null"
                   android:ems="10"
                   android:hint="(选填)：如，长、宽、高、件数、是否需要雨布等"
                   android:imeOptions="actionNext"
                   android:paddingLeft="5dp"
                   android:singleLine="true"
                   android:textSize="12sp" />

               <TextView
                   android:id="@+id/ee"
                   android:layout_width="wrap_content"
                   android:layout_height="wrap_content"
                   android:layout_centerVertical="true"
                   android:layout_marginLeft="20dp"
                   android:text="备注：" />

               </RelativeLayout>
                  <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                 android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginTop="8dp"
                 android:background="@drawable/corners_bg"
                >
		<TextView 
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_marginLeft="3dp"
		     android:textColor="@color/orange"
		    android:text="友情提示：如需拆卸、安装、上楼搬货的额外服务，请在备注中写明。"
		    />
         </RelativeLayout>
              </LinearLayout>
</LinearLayout>
</RelativeLayout>
</LinearLayout>
