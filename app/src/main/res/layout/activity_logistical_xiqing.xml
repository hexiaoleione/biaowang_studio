<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
<RelativeLayout
        android:id="@+id/infor_shows"
            android:layout_width="match_parent"
            android:layout_height="50dip"
            android:background="#fa7024"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/btnLeft"
                android:layout_width="60dp"
                android:layout_height="50dp"
                android:layout_centerVertical="true"
                android:contentDescription="@null"
                android:paddingBottom="14dp"
                android:paddingLeft="24dp"
                android:paddingRight="14dp"
                android:paddingTop="14dp"
                android:src="@drawable/back1" />

            <ImageView
                android:id="@+id/btnRight"
                android:layout_width="28dp"
                android:layout_height="28dp"
                android:layout_alignBaseline="@+id/btnLeft"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_marginRight="16dp"
                 />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:text="镖王报价"
                android:textColor="@color/whites"
                android:textSize="16sp" />
        </RelativeLayout>
    <ScrollView 
       android:layout_width="match_parent"
       android:layout_height="match_parent">
   <LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white"
    > 
<LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:layout_marginTop="10dp"
    >
    <TextView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="货物信息"
        android:textSize="13sp"
        />
</LinearLayout>
	<View 
	    android:layout_width="match_parent"
	    android:layout_height="1dp"
	    android:layout_marginTop="5dp"
	    android:background="#E1E1E1"
    
    />

<LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
     android:layout_marginTop="5dp"
    >
   
      <TextView 
          android:id="@+id/spec_huoname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="名字"
        android:textSize="13sp"
             android:layout_weight="1"
        />
      
</LinearLayout>
<TextView 
          android:id="@+id/spec_jiazhi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:textSize="13sp"
           android:layout_marginTop="5dp"
           android:visibility="gone"
        android:text="价值"
        />
<TextView
          android:id="@+id/spec_fahuodizhi"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
         android:layout_marginLeft="10dp"
           android:textSize="13sp"
           android:layout_marginTop="5dp"
          android:text="发货地" />
<LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="5dp"
    android:layout_marginLeft="10dp"
    >
    <TextView
        android:id="@+id/spec_dadaodizhi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
           android:layout_alignBaseline="@+id/spec_songhuo"
          android:layout_alignBottom="@+id/spec_songhuo"
         android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/textView2"
        android:textSize="13sp"
        android:text="目的地" />
    
</LinearLayout>
<TextView
            android:id="@+id/spec_fahuotime"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_alignParentTop="true"
           android:layout_marginLeft="10dp"
            android:text="发货时间"
            android:layout_marginTop="5dp"
            android:visibility="gone"
            android:textSize="13sp" />
<LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
     android:layout_marginTop="5dp"
    >
<TextView
        android:id="@+id/spec_dadaotime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="送到时间"
        android:layout_weight="1"
        android:textSize="13sp" />
   <TextView 
          android:id="@+id/spec_jian"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="13sp"
        android:text="件数"
        android:layout_weight="1"
        />
      
</LinearLayout>
<LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
     android:layout_marginTop="5dp"
    >
    <TextView 
        android:id="@+id/spec_zhong"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="货物重量"
        android:layout_weight="1"
        android:textSize="13sp"
        />
      <TextView 
          android:id="@+id/spec_tiji"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:textSize="13sp"
        android:text="货物体积"
        android:layout_weight="1"
        />
      
</LinearLayout>

<LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
     android:layout_marginTop="10dp"
    >

      <TextView
          android:id="@+id/spec_quhuo"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_alignParentLeft="true"
          android:layout_alignParentTop="true"
          android:text="需要取货"
           android:layout_weight="1"
          android:textColor="@color/orange1"
          android:textSize="13sp" />

        <TextView
        android:id="@+id/spec_songhuo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:text="需要送货"
         android:layout_weight="1"
        android:textColor="@color/orange1"
        android:textSize="13sp" />
      
</LinearLayout>



<LinearLayout 
    android:id="@+id/spec_shouxin"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
     android:layout_marginTop="10dp"
     android:visibility="gone"
    >
    <TextView 
        android:id="@+id/spec_shouhuoname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="收货人"
        android:textSize="13sp"
        />
      <TextView 
          android:id="@+id/spec_lianxifangshi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:text="联系方式"
        android:textSize="13sp"
        />
      
</LinearLayout>
<LinearLayout 
    android:id="@+id/spec_faxin"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
     android:layout_marginTop="10dp"
      android:visibility="gone"
    >
    <TextView 
        android:id="@+id/spec_fahuoname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="发货人"
        android:textSize="13sp"
        />
      <TextView 
          android:id="@+id/spec_falianxifangshi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="5dp"
        android:text="发联系方式"
        android:textSize="13sp"
        />
      
</LinearLayout>
<LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
     android:layout_marginTop="10dp"
    >
</LinearLayout>
  <LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
    >
    <TextView 
        android:id="@+id/spec_beizhu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="备注"
        />
</LinearLayout>
                 <View
                        android:layout_width="match_parent"
                        android:layout_marginTop="10dp"
                        android:layout_height="5dp"
                        android:background="@color/baseBgColor" />
                 
       <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="match_parent"
          android:background="@color/graywhite"
           android:orientation="vertical" >

    <TextView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         android:layout_marginLeft="10dp"
        android:text="备注："
        />

    <EditText
        android:id="@+id/spec_beizhus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/rounded_edittext"
        android:layout_marginRight="10dp"
         android:layout_marginLeft="10dp"
         android:layout_marginTop="5dp"
         android:hint=" 请输入您的预计时间：如
         预计运输时间：10小时 "
         android:lines="3"
         android:textSize="12sp"
         android:paddingLeft="5dp"
        android:gravity="top|left"
        android:ems="30" >
    </EditText>
    
    <RelativeLayout 
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        
        >
    <TextView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         android:layout_marginLeft="10dp"
         android:layout_marginTop="10dp"
        android:text="货场地址："
        android:id="@+id/textView9"/>

    <ImageView
        android:id="@+id/add_receiver"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_alignParentBottom="true"
        android:layout_toRightOf="@+id/textView9"
        android:background="@drawable/add_oreg" />

  </RelativeLayout>
    <EditText
        android:id="@+id/spec_huoaddse"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/rounded_edittext"
        android:layout_marginRight="10dp"
         android:layout_marginLeft="10dp"
         android:layout_marginTop="5dp"
         android:hint=" "
         android:lines="3"
         android:textSize="12sp"
         android:paddingLeft="5dp"
         android:visibility="gone"
        android:gravity="top|left"
        android:ems="30" >
    </EditText>
<TextView 
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
         android:layout_marginLeft="10dp"
         android:layout_marginTop="10dp"
        android:text="报价："
        />

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/corners_bg" >
   <RelativeLayout 
       android:id="@+id/r1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
       >
        <EditText
            android:id="@+id/editText1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/textView1"
             android:inputType="number"
             android:background="@drawable/edit_bg"
             android:paddingLeft="3dp"
            android:ems="3" >
        </EditText>
        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/editText1"
            android:textSize="12sp"
            android:text="元" />
        <TextView
            android:id="@+id/textView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
                 android:textSize="12sp"
                 android:layout_marginLeft="10dp"
            android:text="上门取货" />


   </RelativeLayout>
   <RelativeLayout 
       android:id="@+id/r3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/r2"
           android:layout_alignParentLeft="true"
       android:layout_marginTop="5dp"
       >
        <EditText
             android:id="@+id/spec_edt_yuan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_toRightOf="@+id/textView5"
            android:inputType="number"
            android:paddingLeft="3dp"
             android:background="@drawable/edit_bg"
            android:ems="3" >
        </EditText>
        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:layout_toRightOf="@+id/spec_edt_yuan"
                 android:textSize="12sp"
            android:text="元" />
        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerVertical="true"
            android:textSize="12sp"
             android:layout_marginLeft="10dp"
            android:text="        运费" />


   </RelativeLayout>
   <RelativeLayout 
       android:id="@+id/r4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/r3"
         android:layout_marginBottom="5dp"
       >

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="13dp"
            android:text="元"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/text_total"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignRight="@+id/textView8"
            android:layout_marginRight="15dp"
            android:textColor="@color/orange1"
            android:text="" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBaseline="@+id/text_total"
            android:layout_alignBottom="@+id/text_total"
            android:layout_marginRight="5dp"
            android:layout_toLeftOf="@+id/text_total"
            android:text="合计："
            android:textSize="12sp" />

    </RelativeLayout>

   <RelativeLayout
       android:id="@+id/r2"
       android:layout_width="wrap_content"
       android:layout_height="wrap_content"
       android:layout_alignParentLeft="true"
       android:layout_marginTop="5dp"
       android:layout_below="@+id/r1"
        >

       <EditText
           android:id="@+id/editText2"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_alignParentTop="true"
           android:layout_toRightOf="@+id/textView3"
           android:background="@drawable/edit_bg"
           android:ems="3"
           android:inputType="number"
           android:paddingLeft="3dp" />

       <TextView
           android:id="@+id/textView4"
           android:layout_width="wrap_content"
           android:layout_height="25dp"
           android:layout_centerVertical="true"
           android:layout_toRightOf="@+id/editText2"
           android:text="元"
           android:textSize="12sp" />

       <TextView
           android:id="@+id/textView3"
           android:layout_width="wrap_content"
           android:layout_height="wrap_content"
           android:layout_centerVertical="true"
           android:layout_marginLeft="10dp"
           android:text="送货上门"
           android:textSize="12sp" />
   </RelativeLayout>

    </RelativeLayout>

    <Button
        android:id="@+id/deta_but"
        style="@style/btn"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="20dp"
        android:text="提交报价" />

</LinearLayout> 

</LinearLayout>
</ScrollView>

</LinearLayout>