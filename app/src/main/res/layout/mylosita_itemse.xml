<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/graywhite"
    android:orientation="vertical" >
    <LinearLayout 
        android:id="@+id/infor_title"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    >
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="5dp"
            android:orientation="vertical" 
            >
            <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
           android:layout_marginRight="5dp"
            android:orientation="horizontal"
            >
                  <TextView
                  android:id="@+id/infor_title_time"
                android:layout_width="fill_parent"
                 android:layout_centerVertical="true"
                android:layout_height="20dp"
                android:layout_marginLeft="5dp"
                android:text="dizhi"
                android:textSize="13sp"
                 />

                 	<TextView
                 	       android:id="@+id/infor_title_juli"
                 	    android:layout_width="wrap_content"
                 	     android:layout_height="15dp"
                 	    android:layout_alignParentRight="true"
                 	    android:layout_alignParentTop="true"
                 	    android:layout_marginRight="23dp"
                 	    android:gravity="bottom"
                 	     android:layout_centerVertical="true"
                 	     android:textSize="13sp"
                 	    android:text="20km" />
<Button
    android:layout_width="30dp"
    android:layout_height="30dp"
         android:layout_centerVertical="true"
    android:layout_centerHorizontal="true"
    android:background="@drawable/wi_carimg110"
    android:visibility="gone" />
                 </RelativeLayout>
                 <View 
                     android:layout_width="match_parent"
                     android:layout_height="1dp"
                     android:background="@color/grayview"
                     />
                 
                 <TextView
                android:id="@+id/infor_title_wupin"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:gravity="center"
                 android:layout_marginLeft="15dp"
                android:text="物品："
                android:textSize="13sp"
                />
   <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:layout_marginLeft="15dp"
           android:layout_marginRight="15dp"
            android:orientation="horizontal" 
            >
                  <TextView
                android:id="@+id/infor_title_zhongliang"
                android:layout_width="fill_parent"
                android:layout_height="20dp"
                android:text="重量"
                android:textSize="13sp"
                 />

                 	<TextView
                 	     android:id="@+id/infor_title_tiji"
                 	    android:layout_width="wrap_content"
                 	     android:layout_height="15dp"
                 	    android:layout_alignParentRight="true"
                 	    android:layout_alignParentTop="true"
                 	    android:layout_marginRight="23dp"
                 	    android:gravity="bottom"
                 	    android:textSize="13sp"
                 	    android:text="体积" />

                 </RelativeLayout>

      <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" 
            >
 	<LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            android:layout_marginLeft="10dp"
            android:orientation="vertical" 
            >
             <ImageView 
                 android:layout_width="20dp"
            android:layout_height="20dp"
            android:background="@drawable/star_green44"
                 />    
                 <ImageView 
                 android:layout_width="20dp"
               android:layout_height="20dp" 
               android:layout_marginTop="5dp"
                android:background="@drawable/down_red44"
                 />  
                 </LinearLayout>
		<LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" 
            >
 		<RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="25dp"
            android:orientation="horizontal" 
            >
                  <TextView
                android:id="@+id/infor_title_songhuodizhi"
                android:layout_width="fill_parent"
                android:layout_height="20dp"
                android:text="dizhi"
                android:textSize="13sp"
                 />

                 	<TextView
                 	     android:id="@+id/infor_title_quhuo"
                 	    android:layout_width="wrap_content"
                 	     android:layout_height="15dp"
                 	    android:layout_alignParentRight="true"
                 	    android:layout_alignParentTop="true"
                 	    android:layout_marginRight="23dp"
                 	    android:gravity="bottom"
                 	     android:textColor="@color/orange1"
                 	       android:textSize="13sp"
                 	    android:text="上门取货" />

                 </RelativeLayout>
 		
 		<RelativeLayout
            android:layout_width="wrap_content"
             android:layout_height="25dp"
            android:orientation="horizontal" 
            >
                  <TextView 
                       android:id="@+id/infor_title_daodadizhi"
                android:layout_width="fill_parent"
             android:layout_height="20dp"
            android:layout_marginRight="15dp"
                android:text="ddddddd"
                android:textSize="13sp"
                 />

                 	<TextView
                 	      android:id="@+id/infor_title_songhuo"
                 	    android:layout_width="wrap_content"
                 	     android:layout_height="15dp"
                 	    android:layout_alignParentRight="true"
                 	    android:layout_alignParentTop="true"
                 	    android:layout_marginRight="23dp"
                 	    android:gravity="bottom"
                 	      android:textColor="@color/orange1"
                 	        android:textSize="13sp"
                 	    android:text="送货上门" />

                 </RelativeLayout>
                    </LinearLayout>
        </LinearLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="45dp" >

            <Button
                android:id="@+id/infor_title_baojia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/button3"
                android:layout_alignTop="@+id/button3"
                android:layout_centerHorizontal="true"
                android:background="@drawable/seemoney_bar"
                android:visibility="gone"/>

   <Button
       android:id="@+id/button3"
       android:layout_width="wrap_content"
       android:layout_height="30dp"
       android:layout_alignParentLeft="true"
       android:layout_centerVertical="true"
       android:layout_marginLeft="10dp"
       android:background="@drawable/logisbaojia" />

		<Button
    android:id="@+id/button4"
    android:layout_width="wrap_content"
    android:layout_height="30dp"
    android:layout_centerVertical="true"
    android:layout_centerHorizontal="true"
    android:background="@drawable/logisyifahuo1" />

		<Button
		    android:id="@+id/button5"
		    android:layout_width="wrap_content"
		    android:layout_height="30dp"
		      android:layout_centerVertical="true"
            android:layout_centerHorizontal="true"
		    android:layout_alignParentRight="true"
		    android:layout_marginRight="10dp"
		    android:background="@drawable/logishuowu1" />

<Button
    android:id="@+id/button7"
    android:layout_width="50dp"
    android:layout_height="wrap_content"
    android:layout_centerVertical="true"
    android:layout_toRightOf="@+id/button4"
    android:layout_toLeftOf="@+id/button5"
    android:background="@drawable/grayseach_13880" />

<Button
    android:id="@+id/button6"
    android:layout_width="50dp"
    android:layout_height="wrap_content"
    android:layout_alignParentTop="true"
    android:layout_toRightOf="@+id/button3"
    android:layout_toLeftOf="@+id/button4"
    android:background="@drawable/grayseach_13880" />

        </RelativeLayout>

</LinearLayout>
</LinearLayout>
<LinearLayout 
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
    android:layout_marginRight="10dp"
    android:layout_marginTop="10dp"
    android:background="@drawable/corners_bg"
    android:visibility="gone"
    >
    <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginTop="5dp"
            android:orientation="vertical" 
            >
            <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginLeft="15dp"
           android:layout_marginRight="15dp"
            android:orientation="horizontal" 
            >
                  <TextView
                  android:id="@+id/my_infor_time"
                android:layout_width="fill_parent"
                android:layout_height="15dp"
                android:text="dizhi"
                 />

                 	<TextView
                 	       android:id="@+id/my_infor_juli"
                 	    android:layout_width="wrap_content"
                 	     android:layout_height="15dp"
                 	    android:layout_alignParentRight="true"
                 	    android:layout_alignParentTop="true"
                 	    android:layout_marginRight="23dp"
                 	    android:gravity="bottom"
                 	    android:text="20km" />

                 </RelativeLayout>
                 <View 
    android:layout_width="match_parent"
    android:layout_height="1dp"
    android:layout_marginLeft="15dp"
    android:layout_marginRight="15dp"
    android:background="@color/dividerColor"
    />
                 <TextView
                android:id="@+id/my_infor_wupin"
                android:layout_width="wrap_content"
                android:layout_height="20dp"
                android:gravity="center"
                 android:layout_marginLeft="15dp"
                android:text="物品："
                />
   <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:layout_marginLeft="15dp"
           android:layout_marginRight="15dp"
            android:orientation="horizontal" 
            >
                  <TextView
                android:id="@+id/my_infor_zhongliang"
                android:layout_width="fill_parent"
                android:layout_height="15dp"
                android:text="重量"
                 />

                 	<TextView
                 	     android:id="@+id/my_infor_tiji"
                 	    android:layout_width="wrap_content"
                 	     android:layout_height="15dp"
                 	    android:layout_alignParentRight="true"
                 	    android:layout_alignParentTop="true"
                 	    android:layout_marginRight="23dp"
                 	    android:gravity="bottom"
                 	    android:text="体积" />

                 </RelativeLayout>

      <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal" 
            >
 	<LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:orientation="vertical" 
            >
             <ImageView 
                 android:layout_width="20dp"
            android:layout_height="20dp"
            android:background="@drawable/star_green44"
                 />    
                 <ImageView 
                 android:layout_width="20dp"
               android:layout_height="20dp" 
                android:background="@drawable/down_red44"
                 />  
                 </LinearLayout>
		<LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical" 
            >
 		<RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:orientation="horizontal" 
            >
                  <TextView
                       android:id="@+id/my_infor_songhuodizhi"
                android:layout_width="fill_parent"
                android:layout_height="15dp"
                android:text="dizhi"
                 />

                 	<TextView
                 	     android:id="@+id/my_infor_quhuo"
                 	    android:layout_width="wrap_content"
                 	     android:layout_height="15dp"
                 	    android:layout_alignParentRight="true"
                 	    android:layout_alignParentTop="true"
                 	    android:layout_marginRight="23dp"
                 	    android:gravity="bottom"
                 	     android:textColor="@color/orange1"
                 	    android:text="上门取货" />

                 </RelativeLayout>
 		
 		<RelativeLayout
            android:layout_width="wrap_content"
             android:layout_height="15dp"
            android:orientation="horizontal" 
            >
                  <TextView 
                       android:id="@+id/my_infor_daodadizhi"
                android:layout_width="fill_parent"
             android:layout_height="15dp"
            android:layout_marginRight="15dp"
                android:text="ddddddd"
                 />

                 	<TextView
                 	      android:id="@+id/my_infor_songhuo"
                 	    android:layout_width="wrap_content"
                 	     android:layout_height="15dp"
                 	    android:layout_alignParentRight="true"
                 	    android:layout_alignParentTop="true"
                 	    android:layout_marginRight="23dp"
                 	    android:gravity="bottom"
                 	      android:textColor="@color/orange1"
                 	    android:text="送货上门" />

                 </RelativeLayout>
                    </LinearLayout>
        </LinearLayout>


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="45dp" >

            <Button
                android:id="@+id/my_infor_baojia"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignBottom="@+id/button2"
                android:layout_alignTop="@+id/button2"
                android:layout_centerHorizontal="true"
                android:background="@drawable/seemoney_bar"/>

   <TextView
       android:id="@+id/my_infor_xin"
       android:layout_width="wrap_content"
       android:layout_height="15dp"
       android:layout_alignBaseline="@+id/button2"
       android:layout_alignBottom="@+id/button2"
       android:layout_alignParentRight="true"
       android:layout_marginRight="18dp"
       android:text="121" />

   <Button
       android:id="@+id/button2"
       android:layout_width="30dp"
       android:layout_height="30dp"
       android:layout_centerVertical="true"
       android:layout_toLeftOf="@+id/my_infor_xin"
       android:background="@drawable/hotheat_bar" />

        </RelativeLayout>

</LinearLayout>
</LinearLayout>

</LinearLayout>
